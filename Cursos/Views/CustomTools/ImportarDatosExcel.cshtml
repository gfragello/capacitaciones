
@{
    ViewBag.Title = "Importar datos desde archivo Excel";
}

<h2>Importar datos desde archivo Excel</h2>
<hr />

@using(Html.BeginForm("ImportarDatosExcel", "CustomTools", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <table>
        <tr>
            <td>File:</td>
            <td><input type="file" name="UploadedFile" /></td>
        </tr>
        <tr>
            <td colspan="2">
                <input type="submit" name="Submit" value="Submit" />
            </td>
        </tr>
    </table>
}

